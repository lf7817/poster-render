{
  "name": "taro-poster-render",
  "version": "1.12.0",
  "description": "taro海报组件，兼容企微、支付宝",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "none"
  },
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "src/components/Poster/index.tsx",
  "types": "types/index.d.ts",
  "homepage": "https://github.com/lf7817/taro-poster-render",
  "files": [
    "dist",
    "lib",
    "types"
  ],
  "keywords": [
    "taro",
    "poster",
    "canvas",
    "微信",
    "企微",
    "支付宝"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lf7817/taro-poster-render.git"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "clean": "rm -rf dist/ types/ lib/",
    "prepublish": "npm run clean && npm run build",
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json --declaration --declarationDir types",
    "build:rollup": "rollup --config ./rollup.config.js",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "lf7817 <535536456@qq.com>",
  "lint-staged": {
    "src/**/*.ts?(x)": "eslint --fix",
    "src/**/*.{ts?(x),css,less,scss,md,json,html}": "prettier --write"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.3.14",
    "@tarojs/react": "3.3.14",
    "@tarojs/runtime": "3.3.14",
    "@tarojs/taro": "3.3.14",
    "lodash.isequal": "^4.5.0",
    "p-limit": "^4.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@rollup/plugin-commonjs": "^13.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@tarojs/mini-runner": "3.3.14",
    "@tarojs/webpack-runner": "3.3.14",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.3.14",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-taro": "3.3.14",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "rollup": "^2.60.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "stylelint": "9.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.1.0"
  }
}
