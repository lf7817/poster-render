{
  "name": "taro-free-poster",
  "version": "2.1.4-beta.0",
  "description": "taro海报组件，兼容企微、支付宝",
  "license": "MIT",
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "homepage": "https://github.com/lf7817/taro-poster-render",
  "files": [
    "dist",
    "lib",
    "types"
  ],
  "keywords": [
    "taro",
    "poster",
    "canvas",
    "微信",
    "企微",
    "支付宝"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lf7817/taro-poster-render.git"
  },
  "scripts": {
    "clean": "rm -rf dist/ types/ lib/",
    "prepublish": "npm run clean && npm run build",
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json --declaration --declarationDir types",
    "build:rollup": "rollup --config ./rollup.config.js"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "lf7817 <535536456@qq.com>",
  "dependencies": {
    "p-limit": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.6",
    "rollup-plugin-typescript2": "^0.32.1",
    "stylelint": "^14.9.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "@tarojs/taro": "3.4.13"
  }
}
